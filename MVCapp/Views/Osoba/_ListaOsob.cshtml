@model IEnumerable<MVCapp.Models.Osoba>

<div id="listaOsob-selection">
    <table class="table">
        <tr>
            <th>Imię</th>
            <th>Nazwisko</th>
            <th></th>
        </tr>

        @foreach(var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem=> item.Imie)</td>
                <td>@Html.DisplayFor(modelItem=> item.Nazwisko)</td>
            </tr>
        }
    </table>
</div>